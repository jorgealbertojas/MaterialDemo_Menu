<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TranslucentAppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!--<item name="android:actionBarStyle">@style/Widget.ActionBar</item>-->
        <item name="actionBarStyle">@style/Widget.ActionBar</item>
    </style>

    <style name="TranslucentAppTheme.TranslucentActionBar">
        <!--<item name="android:actionBarStyle">@style/Widget.ActionBar.Transparent</item>-->
        <!--<item name="android:windowActionBarOverlay">true</item>-->
        <item name="actionBarStyle">@style/Widget.ActionBar.Transparent</item>
        <item name="windowActionBarOverlay">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="TranslucentAppTheme.Light.TranslucentActionBar" parent="Theme.AppCompat.Light">
        <!--<item name="android:actionBarStyle">@style/Widget.Light.ActionBar.Transparent</item>-->
        <!--<item name="android:windowActionBarOverlay">true</item>-->
        <item name="actionBarStyle">@style/Widget.Light.ActionBar.Transparent</item>
        <item name="windowActionBarOverlay">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

</resources>